@model Moridge.Models.StatisticsChart

@{
    var chart = new Chart(width: 300, height: 450)
        .AddTitle(@Model.GetTitle())
        .AddSeries(
            name: "Bookings",
            xValue: @Model.Dates,
            yValues: @Model.EventCount)
        .SetXAxis(title: @Model.XAxis)
        .SetYAxis(title: "Antal bokningar")
        .Write();
}